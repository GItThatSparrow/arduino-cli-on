<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="tech.sorethumb.clion.view.ui.ArduinoCLIPreferencesForm">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="780" height="500"/>
    </constraints>
    <properties>
      <enabled value="true"/>
      <maximumSize width="500" height="500"/>
      <minimumSize width="100" height="200"/>
      <preferredSize width="200" height="200"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="eb921" layout-manager="GridLayoutManager" row-count="1" column-count="5" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="North"/>
        <properties>
          <maximumSize width="500" height="500"/>
          <minimumSize width="100" height="50"/>
          <preferredSize width="200" height="50"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="d7cef" class="javax.swing.JLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="API Path"/>
            </properties>
          </component>
          <component id="b2181" class="com.intellij.openapi.ui.TextFieldWithBrowseButton" binding="apiPath">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="3" hsize-policy="6" anchor="8" fill="0" indent="2" use-parent-layout="false">
                <minimum-size width="300" height="-1"/>
                <maximum-size width="500" height="-1"/>
              </grid>
            </constraints>
            <properties>
              <editable value="true"/>
              <enabled value="true"/>
              <text value="&lt;API Path Here&gt;"/>
            </properties>
          </component>
          <component id="c2bce" class="javax.swing.JButton" binding="testPath">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Test"/>
            </properties>
          </component>
          <component id="29a8e" class="javax.swing.JLabel" binding="testArduinoPathResult">
            <constraints>
              <grid row="0" column="3" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value=""/>
            </properties>
          </component>
          <component id="96056" class="com.intellij.util.ui.LabelWithTooltip">
            <constraints>
              <grid row="0" column="4" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <icon value="general/information.png"/>
              <toolTipText value="The API Path is the locaiton where your &quot;Arduino15&quot; directory lives.  This is where the build tools will look for Arduino-specific build tools."/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="92e70" layout-manager="GridLayoutManager" row-count="1" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="Center"/>
        <properties>
          <maximumSize width="500" height="500"/>
          <minimumSize width="100" height="50"/>
          <preferredSize width="200" height="50"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="6ba11" class="javax.swing.JTextPane" binding="useTheUpdateIndexTextPane" default-binding="true">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="9" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <contentType value="text/html"/>
              <editable value="false"/>
              <enabled value="false"/>
              <font name="SansSerif"/>
              <text value="&lt;html&gt;&#10;  &lt;head&gt;&#10;    &#10;  &lt;/head&gt;&#10;  &lt;body&gt;&#10;    &lt;p style=&quot;margin-top: 0&quot;&gt;&#10;      Use the 'Update Index' button to download updates via the &lt;code&gt;arduino-cli&lt;/code&gt;. &#10;      This will overwrite your existing &lt;code&gt;package_index.json&lt;/code&gt; file &#10;      in your installation directory.&#10;    &lt;/p&gt;&#10;  &lt;/body&gt;&#10;&lt;/html&gt;&#10;"/>
            </properties>
            <clientProperties>
              <JEditorPane.honorDisplayProperties class="java.lang.Boolean" value="true"/>
              <JEditorPane.w3cLengthUnits class="java.lang.Boolean" value="false"/>
            </clientProperties>
          </component>
          <component id="b6f99" class="javax.swing.JButton" binding="updateIndexButton" default-binding="true">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="1" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Update Index"/>
            </properties>
          </component>
          <component id="6eb6d" class="com.intellij.util.ui.LabelWithTooltip">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="1" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <icon value="general/information.png"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
